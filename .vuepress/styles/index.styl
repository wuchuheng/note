.codesandbox {
  border: 0;
  border-radius: 4px;
  height: 500px;
  overflow: hidden;
  width: 100%;
}
.search-box .suggestion a .suggestion-row .suggestion-content .highlight {
  color: red;
}
.search-box>input {
  background-color: transparent !important;
  border: 1px solid var(--border-color) !important;
  &:focus {
    border-color: $accentColor !important;
  }
}